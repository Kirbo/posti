{
  "name": "posti",
  "version": "3.4.0",
  "description": "Posti Basic Address auto-updater",
  "repository": "https://github.com/kirbo/posti",
  "author": "Kimmo Saari <kirbo@kirbo-designs.com>",
  "license": "MIT",
  "scripts": {
    "build": "cross-env NODE_ENV=build npx babel src --out-dir dist",
    "start": "babel-node src/posti.js",
    "force": "cross-env force=true babel-node src/posti.js",
    "lint": "eslint .",
    "test": "yarn test:debug --silent",
    "test:debug": "cross-env NODE_ENV=test jest --verbose --detectOpenHandles --forceExit",
    "prepush": "yarn lint && yarn test"
  },
  "engines": {
    "node": ">=6.0.0",
    "yarn": ">=1.3.2"
  },
  "main": "./dist/index.js",
  "bin": {
    "posti": "./dist/posti.js"
  },
  "files": [
    "dist",
    "package.json"
  ],
  "keywords": [
    "posti",
    "finnish post",
    "basic addresses",
    "sequelize",
    "mysql",
    "mariadb",
    "cli"
  ],
  "dependencies": {
    "bluebird": "3.7.2",
    "cli-progress": "3.4.0",
    "colors": "1.4.0",
    "fs-extra": "8.1.0",
    "iconv-lite": "0.5.0",
    "mysql2": "2.0.1",
    "pg": "7.14.0",
    "pg-hstore": "2.3.3",
    "regenerator-runtime": "0.13.3",
    "request": "2.88.0",
    "sequelize": "5.21.2",
    "sqlite3": "4.1.0",
    "tedious": "6.6.2",
    "unzipper": "0.10.5"
  },
  "devDependencies": {
    "@babel/cli": "7.7.4",
    "@babel/core": "7.7.4",
    "@babel/node": "7.7.4",
    "@babel/plugin-proposal-class-properties": "7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "7.7.4",
    "@babel/polyfill": "7.7.0",
    "@babel/preset-env": "7.7.4",
    "babel-eslint": "10.0.3",
    "babel-plugin-minify-constant-folding": "0.5.0",
    "babel-plugin-minify-dead-code-elimination": "0.5.1",
    "babel-plugin-minify-guarded-expressions": "0.4.4",
    "babel-plugin-minify-mangle-names": "0.5.0",
    "babel-plugin-minify-replace": "0.5.0",
    "babel-plugin-minify-simplify": "0.5.1",
    "babel-plugin-shebang": "1.0.0",
    "babel-preset-minify": "0.5.1",
    "cross-env": "6.0.3",
    "eslint": "6.7.2",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-plugin-compat": "3.3.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsdoc": "18.4.1",
    "eslint-plugin-require-jsdoc": "1.0.4",
    "husky": "3.1.0",
    "jest": "24.9.0",
    "jest-plugin-console-matchers": "2.9.0"
  }
}
