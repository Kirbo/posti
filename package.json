{
  "name": "posti",
  "version": "0.1.5",
  "description": "Posti Basic Address auto-updater",
  "repository": "https://github.com/kirbo/posti",
  "author": "Kimmo Saari <kirbo@kirbo-designs.com>",
  "license": "MIT",
  "scripts": {
    "build": "cross-env NODE_ENV=build npx babel src --out-dir dist",
    "start": "babel-node src/posti.js",
    "force": "cross-env force=true babel-node src/posti.js",
    "lint": "eslint .",
    "test": "yarn test:debug --silent",
    "test:debug": "cross-env NODE_ENV=test jest --verbose --forceExit",
    "prepush": "yarn lint && yarn test"
  },
  "engines": {
    "node": ">=8.9.0",
    "yarn": ">=1.3.2"
  },
  "main": "./dist/index.js",
  "bin": {
    "posti": "./dist/posti.js"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "posti",
    "finnish post",
    "basic addresses",
    "graphql",
    "sequelize",
    "mysql",
    "mariadb",
    "cli"
  ],
  "dependencies": {
    "bluebird": "3.5.1",
    "cli-progress": "1.8.0",
    "colors": "1.2.1",
    "fs-extra": "5.0.0",
    "iconv-lite": "0.4.19",
    "mysql2": "1.5.2",
    "pg": "7.4.1",
    "pg-hstore": "2.3.2",
    "request": "2.85.0",
    "sequelize": "4.37.1",
    "sqlite3": "4.0.0",
    "tedious": "2.3.1",
    "unzip": "0.1.11"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.40",
    "@babel/core": "7.0.0-beta.40",
    "@babel/polyfill": "7.0.0-beta.40",
    "babel-cli": "6.26.0",
    "babel-eslint": "8.2.2",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-minify-constant-folding": "0.3.0",
    "babel-plugin-minify-dead-code-elimination": "0.3.0",
    "babel-plugin-minify-guarded-expressions": "0.3.0",
    "babel-plugin-minify-mangle-names": "0.3.0",
    "babel-plugin-minify-replace": "0.3.0",
    "babel-plugin-minify-simplify": "0.3.0",
    "babel-plugin-remove-comments": "2.0.0",
    "babel-plugin-shebang": "0.0.0",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-property-literals": "6.9.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-minify": "0.3.0",
    "babel-preset-stage-2": "6.24.1",
    "cross-env": "5.1.4",
    "eslint": "4.18.2",
    "eslint-config-airbnb": "16.1.0",
    "eslint-plugin-compat": "2.2.0",
    "eslint-plugin-import": "2.9.0",
    "eslint-plugin-jsdoc": "3.5.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.7.0",
    "eslint-plugin-require-jsdoc": "1.0.4",
    "husky": "0.14.3",
    "jest": "22.4.2",
    "jest-plugin-console-matchers": "2.7.0"
  }
}
